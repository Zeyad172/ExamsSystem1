<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.examssystem.autoExamController">
   <children>
      <Label layoutX="16.0" layoutY="86.0" text="number of easy questions" />
      <Label layoutX="14.0" layoutY="130.0" text="number of medium questions" />
      <Label layoutX="15.0" layoutY="172.0" text="number pf hard questions" />
      <TextField fx:id="easy" layoutX="182.0" layoutY="82.0" />
      <TextField fx:id="medium" layoutX="182.0" layoutY="126.0" />
      <TextField fx:id="hard" layoutX="182.0" layoutY="168.0" />
      <Button fx:id="generate" layoutX="252.0" layoutY="267.0" mnemonicParsing="false" onAction="#generateClicked" text="generate exam" />
      <Label layoutX="36.0" layoutY="52.0" text="Exam Name" />
      <TextField fx:id="examName" layoutX="182.0" layoutY="48.0" />
   </children>
</AnchorPane>
